#include<stdio.h>
void matmul(int a[10][10],int b[10][10],int prod[10][10],int row,int column)
{
for(int i=0;i<row;i++)
{
for(int j=0;j<column;j++)
{
    prod[i][j]=a[i][j]*b[i][j];
}
}}

void print(int prod[10][10],int r,int c)
{
    printf("the sum of two matrices is:\n");
    for(int i=0;i<r;i++)
    {
    for(int j=0;j<c;j++)
    {
        printf("%d ",prod[i][j]);
    }
    printf("\n");
    }
}

void input(int a[10][10],int r,int c)
{
    for(int i=0;i<r;i++)
    {
    for(int j=0;j<c;j++)
    {
        scanf("%d",&a[i][j]);
    }
    }
}

int main()
{
int a[10][10],b[10][10],prod[10][10],m,n;
printf("enter number of rows:");
scanf("%d",&m);
printf("enter number of columns:");
scanf("%d",&n);
printf("Enter values of A:");
input(a,m,n);
printf("Enter values of B:");
input(b,m,n);
matmul(a,b,prod,m,n);
print(prod,m,n);
return 0;
}
