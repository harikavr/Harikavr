#include<stdio.h>
float largenum(float a,float b,float c)
{ int l;
if (a>b&&a>c)
{l=a;
}
else if (a>b&&a>c)
{l=b;
}
else if (c>a&&c>b)
{l=c;
}
return l;}
float printl(int l)
{printf("the largest number is:%d",l);
}
int main()
{ float x,y,z;
printf("enter three numbers:");
scanf("%f%f%f",&x,&y,&z);
int a=largenum(x,y,z);
printl(a);
return 0;
}
